<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutoapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoActionBar">
        <activity android:name=".TutoriasActivity"></activity>
        <activity android:name=".SolicitudActivity" />
        <activity android:name=".CentralActivity" />
        <activity android:name=".PseleccionadoActivity" />
        <activity android:name=".TutorActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".AtributesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Forgotpass_Activity" />
        <activity
            android:name=".Login_Activity"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Register_Activity"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>